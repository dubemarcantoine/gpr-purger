    "jest": "^24.9.0",
    "nodemon": "^1.19.4",
    "pkg": "^4.4.0",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.5.0",
    "tslint": "^5.20.1",
    "tsutils": "^3.17.1",
    "typescript": "^3.7.2"
